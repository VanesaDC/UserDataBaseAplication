<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0" spacing="20.0" style="-fx-background-color: #4db6ac;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/20.0.1" fx:controller="com.UI.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <ImageView fitHeight="448.0" fitWidth="657.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@users.jpg" />
      </image>
   </ImageView>

  <Label fx:id="welcomeText" />
  <Button id="btnIn" onAction="#onHelloButtonClick" style="-fx-background-color: #e91e63;" text="Entrar" textFill="WHITE">
      <font>
         <Font name="System Bold" size="14.0" />
      </font></Button>
</VBox>
